    move_contact_solid(yDirection, abs(ySpeed));
    ySpeed = 0;
    vBool = true;
    if (yDirection = 270) {
        onFloor = true;
    }
} else {
    ySpeed += yGravity; 
    onFloor = false;
}

if ((!hBool and !vBool) and place_meeting(x + xSpeed, y + ySpeed, obj_floor_1)) {
    xSpeed = 0;
}

ySpeed = clamp(ySpeed, -100, yGravityMax);

//Jumperoni
if (onFloor and (keyboard_check_pressed( vk_up ) or keyboard_check_pressed( vk_space ))) {
    ySpeed -= jumpSpeed;
    onFloor = false;
}

// Door Check:
var door = instance_place( x, y, obj_door );
if (door != noone) {
    // We are on a door.
    if (keyboard_check_pressed( vk_up )) {
        room_goto(door.destination);
    }
}

y += ySpeed;

if (onFloor) {
    image_index = 1;
} else {
    image_index = 0;
}

